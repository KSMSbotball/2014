int main()
{
	enable_servos();
	set_servo_position(0,1002);
	msleep(1000);
	
    motor(0,-500);
	motor(2,500);
	msleep(2250);
	
	set_servo_position(0,315);
	msleep(1000);
	
	motor(0,500);
	motor(2,-500);
	msleep(1000);
	
	disable_servos();
	
	return 0;
}
